        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "builder --esm --cjs && tsc",
    "check": "tsc"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@boywithkeyboard/builder": "^0.4.2",
    "@types/node": "^20.10.7",
    "typescript": "^5.3.3"
  }
}
